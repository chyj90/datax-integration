<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyj.datax.mapper.TLogPipelineMapper">

    <resultMap id="BaseResultMap" type="com.cyj.datax.entry.TLogPipeline">
        <id property="seqId" column="seq_id" jdbcType="BIGINT"/>
        <result property="pipelineId" column="pipeline_id" jdbcType="INTEGER"/>
        <result property="startTime" column="start_time" jdbcType="TIMESTAMP"/>
        <result property="endTime" column="end_time" jdbcType="TIMESTAMP"/>
        <result property="status" column="status" jdbcType="BOOLEAN"/>
    </resultMap>

    <sql id="Base_Column_List">
        seq_id,pipeline_id,start_time,
        end_time,status
    </sql>
    <update id="updateStatusAndEndTimeBySeqId">
update t_log_pipeline
 set status = #{status,jdbcType=BOOLEAN},
 end_time = #{endTime}
where
seq_id = #{seqId,jdbcType=NUMERIC}
</update>
</mapper>
